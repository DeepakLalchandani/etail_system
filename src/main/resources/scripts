create table etail_trn.usr
( 
   usr_id varchar(255) primary key, 
   eml varchar(65),
   frst_nm varchar(65),
   lst_nm varchar(65),
   usr_role_typ varchar(1),
   pswrd varchar(65),
   crtd_ts timestamp,
   updtd_ts timestamp
); 
comment on table etail_trn.usr is 'this usr table is used to capture the user registration details for both buyer and seller type users';

comment on column etail_trn.usr.usr_id is 'A unique identifier for the user. This field is the table primary key and cannot be NULL.';

comment on column etail_trn.usr.eml is 'The users email address.';

comment on column etail_trn.usr.frst_nm is 'The users first name.';

comment on column etail_trn.usr.lst_nm is 'The users last name.';

comment on column etail_trn.usr.usr_role_typ is 'The user type to identify whether the user is a Buyer or Seller [B for Buyer and S for seller]';

comment on column etail_trn.usr.pswrd is 'The user password for etailing system login';

comment on column etail_trn.usr.crtd_ts is 'The date and time an instance of user was first created';

comment on column etail_trn.usr.updtd_ts is 'The date and time an instance of user was last updated';
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
